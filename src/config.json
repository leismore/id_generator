{
  "app":
  {
    "projectName":  "id_generator",
    "appID":        "UUID_OF_THIS_APP_ID",
    "host":         "localhost",
    "port":         "YOUR_HOST_PORT",
    "backlog":       511,
    "domain":       "YOUR-DOMAIN.COM"
  },

  "auth_app_self":
  {
    "appID":         "UUID_OF_AUTH_APP_SELF",
    "api":
    {
      "authen": {
        "url": "HTTPS://AUTHENTICATION_URL",
        "timeout": 10000
      },
      "author": {
        "url": "HTTPS://AUTHORIZATION_URL",
        "timeout": 10000
      }
    }
  },

  "myorg":
  {
    "orgID": "UUID_OF_YOUR_ORG"
  },

  "couchdb":
  {
    "designName":   "id_generator",
    "dbPrefix":     "lmos"
  },

  "api":
  {
    "baseURL": "/api/",
    "token":  {
      "url":     "token",
      "timeout": 10000,
      "bitSize": 128,
      "baseEncoding": "123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz"
    },
    "numeric":  {
      "url":     "numeric-id",
      "timeout": 10000
    },
    "short":  {
      "url":     "short-id",
      "timeout": 10000,
      "characters": "0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ-_",
      "worker":     0,
      "seed":       1
    },
    "uuidRand":  {
      "url":     "uuid-rand",
      "timeout": 10000
    },
    "uuidNS":  {
      "url":     "uuid-ns",
      "timeout": 10000
    }
  }
}
